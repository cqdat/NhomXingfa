@model NhomXingfa.Models.NewsViewModel

@{
    ViewBag.Title = Model.category.CategoryName;
}

<section class="banner_area">
    <div class="banner_inner d-flex align-items-center">
        <div class="container">
            <div class="banner_content d-md-flex justify-content-between align-items-center">
                <div class="mb-3 mb-md-0">
                    <h2>Tin tức</h2>
                    <p>@Model.category.CategoryName</p>
                </div>
                <div class="page_link">
                    <a href="/">Trang Chủ</a>
                    <a href="javascript:">@Model.category.CategoryName </a>
                </div>
            </div>
        </div>
    </div>
</section>

@Html.Partial("_index", Model)



@section scripts
{
    <script>
        $(document).ready(function () {

            $(".pull-right li").removeClass("active");

            $(".pull-right li#menutt").addClass("active");

        });
    </script>
}